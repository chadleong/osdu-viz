owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Geologic Modeler","Description":"One who constructs multidimensional models to representing the geology in the subsurface","Code":"Geologic Modeler","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--WorkflowPersonaType:GeomechanicSpecialist","kind":"osdu:wks:reference-data--WorkflowPersonaType:1.0.1","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Geomechanic Specialist","Description":"One who measures, models and predicts the mechanical response of rocks and soils to various stress and pressure conditions at a wide range of scales from plates to pores.","Code":"GeomechanicSpecialist","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--WorkflowPersonaType:Geoscientist","kind":"osdu:wks:reference-data--WorkflowPersonaType:1.0.1","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Geoscientist","Description":"One who studies the history, composition, and processes that make up earth often recorded in the rocks","Code":"Geoscientist","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--WorkflowPersonaType:Micropaleontologist","kind":"osdu:wks:reference-data--WorkflowPersonaType:1.0.1","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Micropaleontologist","Description":"Persona specialising in the study of microscopic fossils, such as foraminifera and ostracods, to determine the age, depositional environment, and potential for oil and gas resources in sedimentary rock formations","Code":"Micropaleontologist","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--WorkflowPersonaType:OperationsGeologist","kind":"osdu:wks:reference-data--WorkflowPersonaType:1.0.1","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Operations Geologist","Description":"One who develops a drilling & evaluation program that will allow the well to reach, evaluate, and if applicable, complete in the geologic objectives in a safe, economical, and environmentally sound manner as part of a multi discipline team ","Code":"OperationsGeologist","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--WorkflowPersonaType:Paleontologist","kind":"osdu:wks:reference-data--WorkflowPersonaType:1.0.1","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Paleontologist","Description":"Persona specialised in the study of fossilized plants and animals to determine the age, depositional environment, and potential for oil and gas resources in sedimentary rock formations","Code":"Paleontologist","Source":"Workbook ReferenceValues/Published/WorkflowPersonaType.1.xlsx; commit SHA 56f7c736."