{"kind":"osdu:wks:Manifest:1.0.0","ReferenceData":[{"id":"{{NAMESPACE}}:reference-data--SamplePreparationMethod:WaterWash","kind":"osdu:wks:reference-data--SamplePreparationMethod:1.0.0","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Water Wash","ID":"WaterWash","Description":"Washing rock samples involves thorough rinsing with distilled water to remove loose particles, dust, and contaminants, often complemented by gentle scrubbing and, if necessary, ultrasonic or acid treatments.","Code":"WaterWash","Source":"Workbook ReferenceValues/Published/SamplePreparationMethod.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--SamplePreparationMethod:AcidWash","kind":"osdu:wks:reference-data--SamplePreparationMethod:1.0.0","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Acid Wash","ID":"AcidWash","Description":"The acid wash involves immersing the rock sample in a dilute acid solution, such as hydrochloric acid, to dissolve and remove soluble minerals, followed by thorough rinsing with distilled water.","Code":"AcidWash","Source":"Workbook ReferenceValues/Published/SamplePreparationMethod.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--SamplePreparationMethod:AirDrying","kind":"osdu:wks:reference-data--SamplePreparationMethod:1.0.0","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Air Drying","ID":"AirDrying","Description":"The rock sample is left to air dry naturally, allowing moisture to evaporate slowly from its surface","Code":"AirDrying","Source":"Workbook ReferenceValues/Published/SamplePreparationMethod.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--SamplePreparationMethod:OvenDrying","kind":"osdu:wks:reference-data--SamplePreparationMethod:1.0.0","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Oven Drying","ID":"OvenDrying","Description":"Involves placing the washed samples in an oven at a low temperature to facilitate efficient and controlled removal of moisture","Code":"OvenDrying","Source":"Workbook ReferenceValues/Published/SamplePreparationMethod.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}},{"id":"{{NAMESPACE}}:reference-data--SamplePreparationMethod:WaterJetCutting","kind":"osdu:wks:reference-data--SamplePreparationMethod:1.0.0","acl":{"owners":["{{DATA_OWNERS_GROUP}}"],"viewers":["{{DATA_VIEWERS_GROUP}}"]},"legal":{"legaltags":["{{LEGAL_TAG}}"],"otherRelevantDataCountries":["{{ISO_3166_ALPHA_2_CODE}}"]},"data":{"Name":"Water Jet Cutting","ID":"WaterJetCutting","Description":"Water jet cutting uses a high-pressure stream of water mixed with abrasive particles to cut through rock samples","Code":"WaterJetCutting","Source":"Workbook ReferenceValues/Published/SamplePreparationMethod.1.xlsx; commit SHA 56f7c736.","CommitDate":"2024-02-09T16:51:15+00:00"}}]}