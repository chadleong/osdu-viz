      "data": {
        "Name": "Cambridge Electromagnetic Viscometer",
        "ID": "CambridgeElectromagneticViscometer",
        "Description": "This method uses electromagnetic force to drive a piston through the fluid in a measurement cell. The resistance to the piston's motion, caused by the fluid's viscosity, is measured to determine the viscosity. It is known for its precision and ability to handle small sample volumes.",
        "Code": "CambridgeElectromagneticViscometer",
        "Source": "Workbook ReferenceValues/Published/ViscosityMethod.1.xlsx; commit SHA c0fd3250.",
        "CommitDate": "2025-02-13T11:59:38+00:00"
      }
